{
  "name": "joplin-plugin-markdown-table-sortable",
  "version": "1.2.2",
  "scripts": {
    "dist": "webpack --env joplin-plugin-config=buildMain && webpack --env joplin-plugin-config=buildExtraScripts && webpack --env joplin-plugin-config=createArchive",
    "prepare": "npm run dist",
    "update": "npm install -g generator-joplin && yo joplin --node-package-manager npm --update --force",
    "updateVersion": "webpack --env joplin-plugin-config=updateVersion"
  },
  "license": "MIT",
  "keywords": [
    "joplin-plugin",
    "markdown",
    "table",
    "sorting",
    "sortable"
  ],
  "homepage": "https://github.com/hieuthi/joplin-plugin-markdown-table-sortable",
  "repository": {
    "type": "git",
    "url": "https://github.com/hieuthi/joplin-plugin-markdown-table-sortable.git"
  },
  "devDependencies": {
    "@codemirror/autocomplete": "^6.18.6",
    "@codemirror/view": "^6.38.1",
    "@types/node": "^18.7.13",
    "chalk": "^4.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "on-build-webpack": "^0.1.0",
    "tar": "^6.1.11",
    "ts-loader": "^9.3.1",
    "typescript": "^4.8.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "yargs": "^16.2.0"
  },
  "files": [
    "publish"
  ]
}
